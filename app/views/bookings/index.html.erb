<div class="row bookings-table">
  <div class="col-sm-12 col-md-6">
    <h3>Bookings</h3>
      <table class="table table-bordered">
        <thead>
          <th>Therapist</th>
          <th>Starting Date</th>
          <th>End time</th>
          <th>Status</th>
          <th>Days to start</th>
          <th>Duration</th>
          <th>View</th>
          <% if current_user.therapist %>
            <th>Confirm</th>
          <% end %>
          <th>Delete</th>
        </thead>
        <tbody class="table-body">
          <% @bookings.each do |booking| %>
            <%= render 'table', booking: booking %>
            <!-- Render partial of table of the booking -->
          <% end %>
        </tbody>
      </table>
  <div id="bookings-index-btn">
    <%= link_to "Browse Therapist", users_path, class: "btn btn-primary" %>
  </div>
  </div>
</div>


